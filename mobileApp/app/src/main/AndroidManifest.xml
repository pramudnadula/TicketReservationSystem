<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ticketing_App"
        tools:targetApi="31">
        <activity
            android:name=".Add_Reservation"
            android:exported="false" />
        <activity
            android:name=".Edit_Profile"
            android:exported="false" />
        <activity
            android:name=".Summary"
            android:exported="false" />
        <activity
            android:name=".Your_Reservations"
            android:exported="false" />
        <activity
            android:name=".My_Profile"
            android:exported="false" />
        <activity
            android:name=".Reservation"
            android:exported="false"
            android:label="Book your Ticket"
            android:parentActivityName=".Main_Menu" />
        <activity
            android:name=".Register"
            android:exported="false" />
        <activity
            android:name=".Main_Menu"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Ticketing_App">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>